[pull]
	rebase = true
[core]
	excludesfile = /Users/carlos.silva/.gitignore
[commit]
	template = /Users/carlos.silva/.gitmessage.txt
[gui]
	pruneduringfetch = true
[smartgit "submodule"]
	fetchalways = false
	update = true
	initializenew = true
[push]
	recurseSubmodules = on-demand
[includeIf "gitdir:~/workspace/"]
    path = ~/workspace/.gitconfig
[includeIf "gitdir:~/myspace/"]
    path = ~/myspace/.gitconfig
[init]
	defaultBranch = main
